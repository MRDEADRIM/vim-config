syntax on
colorscheme habamax
set relativenumber number
set cursorline
set hlsearch
set spell
set wildmenu
set nowrap
filetype indent on

set wildignore+=*/vendor/*,*/DESIGN,

" Netrw Configs
let g:netrw_banner = 0
let g:netrw_liststyle = 3

" Key Bindings
source ~/.vim/custom/key_bindings.vim

" Abbreviation
source ~/.vim/custom/abbreviations.vim

